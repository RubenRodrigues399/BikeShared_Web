{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "app/pageNotFound.jsx", "app/not-found.jsx", "app/actions/estacao/listarEstacoes.js", "app/actions/ciclista/listarCiclistas.js", "app/auth/authProviders.js", "app/actions/estacao/addEstacao.js", "app/actions/admin/addAdmin.js"],
  "exclude": ["node_modules"]
}
